{
  "name": "@inkline/unocss",
  "version": "1.0.0",
  "scripts": {
    "prepare:dev": "cp src/playground/index.template.html src/playground/index.html",
    "prepare:build": "node src/playground/generate.js",
    "start": "npm run build",
    "dev": "npm run prepare:dev && vite",
    "build": "unbuild",
    "build:vite": "npm run prepare:build && vite build",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "1.1.4",
    "@inkline/config": "^1.7.10",
    "@inkline/plugin": "^1.0.3",
    "@types/shelljs": "0.8.11",
    "chokidar": "^3.5.3",
    "eslint": "8.25.0",
    "jsdom": "^20.0.3",
    "np": "^7.6.2",
    "npm-run-all": "4.1.5",
    "pathe": "^0.3.9",
    "sass": "^1.55.0",
    "shelljs": "0.8.5",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4",
    "unbuild": "0.9.1",
    "vite": "^3.1.8",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.7.5",
    "vitest": "0.24.3"
  },
  "dependencies": {
    "@grozav/utils": "1.1.4",
    "unocss": "^0.45.29",
    "unplugin": "^0.9.6"
  },
  "peerDependencies": {
    "@inkline/config": ">=1"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "jsdelivr": "index.cjs",
  "unpkg": "index.cjs",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  }
}
